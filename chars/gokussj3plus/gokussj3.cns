getpower = 50,15                 ;Power to get on hit, on guard
;                                ;(Def Hit: 0)  (Def Guard: 1/2 of hit power)
;givepower = 15,8                ;Power to give opponent on hit, on guard
;                                ;(Def Hit: 0)  (Def Guard: 1/2 of hit power)
