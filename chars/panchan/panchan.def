; Definition file for kfm
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Pan Dragonball GT por Ssonic"  ;Name of character
displayname = "Pan"
versiondate = 07,31,2000    ;Version date of character
mugenversion = 07,31,2000   ;Version of M.U.G.E.N character works on
author = "Ssonic"         ;Character author name
pal.defaults = 1,2,4,5      ;Default palettes in order of preference

; Files for the player
[Files]
